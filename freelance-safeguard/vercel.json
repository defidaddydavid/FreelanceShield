{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "routes": [
    { "src": "/api/test", "dest": "/api/test.js" },
    { "src": "/api/waitlist", "dest": "/api/waitlist-signup.js" },
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
